{
    "version": 2,
    "build": {
    "env": {
        "PRISMA_GENERATE": "true"
    }
    },
    "builds": [
        {
            "src": "src/main.tsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.js"
        }
    ]
}